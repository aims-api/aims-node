{
	"name": "@aims-api/aims-node",
	"version": "0.0.28",
	"license": "UNLICENSED",
	"homepage": "https://github.com/aims-api/aims-node",
	"author": "AIMS API <support@aimsapi.com> (https://aimsapi.com/)",
	"keywords": [
		"music search",
		"similarity search",
		"lyrics search",
		"api"
	],
	"repository": {
		"type": "git",
		"url": "git://github.com/aims-api/aims-node.git"
	},
	"bugs": {
		"url": "https://github.com/aims-api/aims-node/issues"
	},
	"main": "dist/index.js",
	"scripts": {
		"clean": "rimraf dist",
		"start": "node ./bin/index.js",
		"dev": "ts-node ./src/index.ts",
		"prebuild": "yarn run clean",
		"build": "tsc --target es2018 --project tsconfig.build.json",
		"lint": "eslint '*/**/*.{js,jsx,ts,tsx}' && tsc --noEmit",
		"fix": "eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
		"ci-lint": "eslint '*/**/*.{js,jsx,ts,tsx}' --quiet",
		"test": "echo 'RUNNING UNIT TESTS' && jest --runInBand",
		"test-integration": "jest -c jest.config.integration.js --runInBand",
		"publish-npm": "yarn build && changeset publish",
		"rm-old-dist": "rm -rf ~/aims-app/apps/main/node_modules/@aims-api/aims-node/dist",
		"cp-new-dist": "cp -r ~/aims-node/dist ~/aims-app/apps/main/node_modules/@aims-api/aims-node/dist",
		"try-build": "yarn build && yarn rm-old-dist && yarn cp-new-dist"
	},
	"dependencies": {
		"axios": "1.7.4",
		"form-data": "^4.0.0",
		"typescript": "^5.1.6",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.3",
		"@changesets/cli": "^2.27.7",
		"@types/node": "^20.4.5",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@typescript-eslint/parser": "^6.2.0",
		"eslint": "^8.46.0",
		"eslint-plugin-import": "^2.28.0",
		"eslint-plugin-n": "^16.0.1",
		"eslint-plugin-promise": "^6.1.1",
		"jest": "29.7.0",
		"msw": "1.3.3",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1"
	},
	"description": "Node.js library for AIMS API",
	"directories": {
		"test": "test"
	},
	"publishConfig": {
		"access": "public"
	}
}
